{
    "compilerOptions": {
        "allowJs": true,
        "allowUnreachableCode": false,
        "baseUrl": ".",
        "checkJs": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "outDir": "./built",
        "paths": {
            "hackingSuite/*": ["hackingSuite/"]
        },
        "target": "ES6",
        "typeRoots": ["./typings"]
    },
    "exclude": ["./built/*"],
    "files": ["typings/index.d.ts"],
    "include": ["hackingSuite/**/*", "typings/index.d.ts", "start.js", "launcher.js"]
}